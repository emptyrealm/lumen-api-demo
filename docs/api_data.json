          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "parts.secondaryCategoryCode",
            "defaultValue": "null",
            "description": "<p>二级分类code</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "parts.descCN",
            "description": "<p>中文描述</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "parts.descEN",
