          },
          {
            "group": "Success 200",
            "type": "String[]",
            "optional": false,
            "field": "items",
            "description": "<p>型号列表</p>"
          },
          {
            "group": "Success 200",
            "type": "String",
            "optional": false,
            "field": "items._icmallPartId",
